class Solution {
public:
    int countPrimes(int n) {
        vector<bool>a(n+1,true);
        int cnt=0;
        a[0]=a[1]=false;
        for(int i=2;i<n;i++){
            if(a[i]){
                cnt++;
                for(int j=2*i;j<n;j=j+i){
                    a[j]=0;
                }
            }
        }
        
        return cnt;
    }
};
